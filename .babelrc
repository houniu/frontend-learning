{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "last 4 versions",
            "not ie < 9",
            "chrome >= 40",
            "safari >= 8",
            "Firefox > 40"
          ]
        },
        "useBuiltIns": false,
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-object-assign",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    "@babel/plugin-proposal-object-rest-spread",
    ["import", {
      "libraryName": "vant",
      "libraryDirectory": "es",
      "style": true
    }],
    "lodash"
  ]
}
