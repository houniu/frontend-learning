{
  "name": "frontend-learning",
  "version": "1.0.0",
  "description": "frontend-learning manage side's module",
  "main": "index.js",
  "scripts": {
    "local": "webpack-dev-server --config ./config/build/webpack.local.conf.js --watch --progress --compress",
    "serve": "DEBUG=koa* NODE_ENV=production nodemon ./index.js",
    "build:local": "webpack --config ./config/build/webpack.local.conf.js",
    "build": "NODE_ENV=production webpack --config ./config/build/webpack.production.conf.js",
    "build:analysis": "NODE_ENV=production BUILD_ANALYSIS=true webpack --config ./config/build/webpack.production.conf.js",
    "lint": "eslint src --ext .js,jsx && stylelint \"src/**/*.less\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/houniu/frontend-learning.git"
  },
  "keywords": [
    "Vue"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{spec,test}.js": [
      "jest"
    ]
  },
  "author": "rdSehunnnie",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/houniu/frontend-learning/issues"
  },
  "homepage": "https://github.com/houniu/frontend-learning#readme",
